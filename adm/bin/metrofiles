#!/bin/sh
# -[Tue Oct  5 17:21:31 2004 by cxh]-
# Return all the source files (with exceptions) in the mescal tree

find . -type f -print | grep -v SCCS | grep -v shilman | grep -v devel | grep -v adm  | grep -v codeDoc | grep -v ptolemy/filter | grep -v com/microstar | grep -v diva | grep -v CVS | grep -v collections | grep -v /cg/ | grep -v '/metro.[0-9]*.tmp'
