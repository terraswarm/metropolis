package producerconsumer;

import metamodel.plt.polychronyscaled.*;

//////////////////////////////////////////////////////////////////////////
//// ProducerConsumer
/** This is the top netlist for the producer consumer example

@author Alessandro Pinto
@version $Id: ProducerConsumer.mmm,v 1.9 2006/10/12 14:21:15 cxh Exp $
*/

public netlist ProducerConsumer extends SynchTopNetlist{

    ////////////////////////////////////////////////////////////////////////////
    ////////////////            Constructor                 ///////////////////

    /**Constructor instances the scheduled netlist and calls the
     * initialization function which creates the whole model.*/

    public ProducerConsumer(String n) {
        super(n);
        ProdConsScheduled prodconsnet = new ProdConsScheduled("ProducerConsumerNetlist");
        initSynchTop(prodconsnet);
    }
}
