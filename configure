PACKAGE_STRING=
PACKAGE_BUGREPORT=

ac_unique_file="mk/metro.mk.in"
ac_default_prefix=$METRO
# Factoring default headers for most tests.
ac_includes_default="\
#include <stdio.h>
#if HAVE_SYS_TYPES_H
# include <sys/types.h>
#endif
#if HAVE_SYS_STAT_H
# include <sys/stat.h>
